{
  "name": "database",
  "version": "0.0.0",
  "private": true,
  "description": "nft.storage database",
  "type": "module",
  "scripts": {
    "setup": "node node ./src/apply-migrations.js",
    "apply-migrations": "node ./src/apply-migrations.js",
    "create-migration": "node ./src/create-migration.js",
    "update-schema": "node ./src/update-schema.js",
    "repl": "node --experimental-repl-await -i -e 'import(`./.repl.js`)'",
    "type-check": "tsc --build",
    "precommit": "lint-staged"
  },
  "license": "(Apache-2.0 AND MIT)",
  "devDependencies": {
    "@fauna-labs/fauna-schema-migrate": "^2.1.2",
    "@types/node": "16.0.0",
    "@types/yargs": "^17.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.2",
    "typescript": "^4.3.4"
  },
  "lint-staged": {
    "*.js": [
      "prettier --no-semi --write",
      "git add"
    ],
    "*.fql": [
      "prettier --write --ignore-unknown",
      "git add"
    ]
  },
  "dependencies": {
    "@nftstorage/ipfs-cluster": "^3.0.1",
    "@web-std/fetch": "^2.0.1",
    "dotenv": "^10.0.0",
    "faunadb": "^4.3.0",
    "subprogram": "^1.2.0",
    "yargs": "^17.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ipfs-shipyard/nft.storage.git",
    "directory": "database"
  },
  "homepage": "https://github.com/ipfs-shipyard/nft.storage/tree/main/database",
  "bugs": "https://github.com/ipfs-shipyard/nft.storage/issues"
}
